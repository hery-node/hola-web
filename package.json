{
  "name": "hola-web",
  "version": "3.0.0",
  "description": "Vue 3 components using with hola-server",
  "author": "hery1979 <hery1979@qq.com>",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "type-check": "vue-tsc --noEmit"
  },
  "main": "./dist/hola-web.umd.cjs",
  "module": "./dist/hola-web.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/hola-web.js",
      "require": "./dist/hola-web.umd.cjs"
    },
    "./style.css": "./dist/style.css",
    "./locales/*": "./dist/locales/*"
  },
  "files": [
    "dist",
    "dist/locales"
  ],
  "dependencies": {
    "axios": "^1.7.0",
    "chartist": "^1.3.0",
    "echarts": "^5.5.0",
    "pinia": "^2.2.0",
    "vue": "^3.5.0",
    "vue-echarts": "^7.0.0",
    "vue-i18n": "^11.2.8",
    "vue-router": "^4.4.0",
    "vuedraggable": "^4.1.0",
    "vuetify": "^3.7.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@mdi/font": "^7.4.0",
    "@types/chartist": "^0.11.1",
    "@types/node": "^22.0.0",
    "@vitejs/plugin-vue": "^5.2.0",
    "@vue/eslint-config-typescript": "^14.0.0",
    "eslint": "^9.0.0",
    "eslint-plugin-vue": "^9.30.0",
    "sass": "^1.80.0",
    "typescript": "~5.6.0",
    "vite": "^6.0.0",
    "vite-plugin-dts": "^4.3.0",
    "vite-plugin-vuetify": "^2.0.0",
    "vue-tsc": "^2.1.0"
  },
  "peerDependencies": {
    "vue": "^3.5.0"
  },
  "bugs": {
    "url": "https://github.com/hery-node/hola-web/issues"
  },
  "homepage": "https://github.com/hery-node/hola-web#readme",
  "keywords": [
    "meta",
    "nodejs",
    "vue",
    "vue3",
    "vuetify",
    "typescript"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hery-node/hola-web.git"
  }
}
